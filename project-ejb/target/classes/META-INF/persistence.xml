<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.2" 
	xmlns="http://xmlns.jcp.org/xml/ns/persistence" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_2.xsd">
  <persistence-unit name="project" transaction-type="JTA">
	  <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
	  <jta-data-source>java:jboss/datasources/sii</jta-data-source>
    <class>es.uma.interneteros.jpa.Cliente</class>
		<class>es.uma.interneteros.jpa.Cuenta_referencia</class>
		<class>es.uma.interneteros.jpa.AutorizacionEmpresaAutorizado</class>
		<class>es.uma.interneteros.jpa.DepositadaPooledReferencia</class>
		<class>es.uma.interneteros.jpa.Pooled</class>
		<class>es.uma.interneteros.jpa.Segregated</class>
		<class>es.uma.interneteros.jpa.Empresa</class>
		<class>es.uma.interneteros.jpa.Individual</class>
		<class>es.uma.interneteros.jpa.Cuenta_Fintech</class>
		<class>es.uma.interneteros.jpa.Transaccion</class>
		<class>es.uma.interneteros.jpa.Divisa</class>
		<class>es.uma.interneteros.jpa.Persona_autorizada</class>
		<class>es.uma.interneteros.jpa.Usuario</class>
		<class>es.uma.interneteros.jpa.Cuenta</class>
    <shared-cache-mode>NONE</shared-cache-mode>
    <properties>
    	<property name="javax.persistence.schema-generation.database.action" value="create"/>
    </properties>
  </persistence-unit>
</persistence>
